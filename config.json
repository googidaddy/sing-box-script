{
  "log": {
    "disabled": false,
    "level": "info",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "tag": "remote",
        "address": "https://1.1.1.1/dns-query",
        "detour": "direct"
      },
      {
        "tag": "local",
        "address": "https://dns.alidns.com/dns-query",
        "detour": "direct"
      }
    ],
    "rules": [
      {
        "domain_suffix": [
          ".cn",
          ".com.cn",
          ".taobao.com",
          ".tmall.com",
          ".alibaba.com",
          ".jd.com",
          ".baidu.com"
        ],
        "server": "local"
      },
      {
        "domain_suffix": [
          ".google.com",
          ".googleapis.com",
          ".youtube.com"
        ],
        "server": "remote"
      },
      {
        "outbound": ["direct"],
        "server": "remote"
      }
    ],
    "strategy": "ipv4_only",
    "independent_cache": true
  },
  "inbounds": [
    {
      "type": "vless",
      "tag": "vless-brutal-sb-in",
      "listen": "::",
      "listen_port": 1234,// 端口
      "sniff": true,
      "sniff_override_destination": true,
      "users": [
        {
          "uuid": "uuid", // uuid 生成
          "flow": ""
        }
      ],
      "tls": {
        "enabled": true,
        "server_name": "偷的域名", // 偷的域名
        "reality": {
          "enabled": true,
          "handshake": {
            "server": "偷的域名", // 偷的域名
            "server_port": 443
          },
          "private_key": "生成的私钥", // 生成的私钥
          "short_id": [
            "id" // 数字id
          ]
        }
      },
      "multiplex": {
        "enabled": true,
        "padding": false,
        "brutal": {
          "enabled": true,
          "up_mbps": 1000,
          "down_mbps": 1000
        }
      }
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct"
    }
  ],
  "route": {
    "rules": [
      {
        "inbound": [
          "vless-brutal-sb-in"
        ],
        "outbound": "direct"
      }
    ]
  }
}